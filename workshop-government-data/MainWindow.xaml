<Window x:Class="workshop_government_data.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:workshop_government_data"
        mc:Ignorable="d"
        Title="Government Data" Height="450" Width="800">
    <Grid Margin="10,10,10,10" RenderTransformOrigin="0.499,0.57">

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Button x:Name="loadBtn" HorizontalAlignment="Center" VerticalAlignment="Center" Height="49" Width="104" Click="ButtonLoadFile" Visibility="Visible">Cargar Archivo</Button>
    </Grid>
</Window>
